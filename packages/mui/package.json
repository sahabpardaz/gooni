  "scripts": {
    "start": "utsdx watch",
    "build": "utsdx build",
    "test:no-jsdom": "utsdx test --passWithNoTests --testPathIgnorePatterns=\"/popover-input/|/overlay-scrollbar/|/check-list-filter/|/date-picker/|/date-range-picker/|/date-range-input/|/time-picker/|/time-range-input/|/time-range-picker/|/toggle-button-input/\"",
    "test:jsdom": "utsdx test --passWithNoTests --env=jsdom --testPathPattern=\"/popover-input/|/overlay-scrollbar/|/check-list-filter/|/date-picker/|/date-range-picker/|/date-range-input/|/time-picker/|/time-range-input/|/time-range-picker/|/toggle-button-input/\"",
    "test": "yarn test:jsdom && yarn test:no-jsdom",
    "prepublishOnly": "yarn test && yarn build",
    "type:check": "yarn tsc",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "chromatic": "chromatic --exit-zero-on-changes"
  },
